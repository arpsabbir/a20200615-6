.add2myApp {
    position: absolute;
    padding-left: 6rpx;
    right: 120rpx;
    width: 200rpx;
    height: 48rpx;
    z-index: 99;
    display: flex;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
    transform: perspective(1000);
}

.add2myApp .bg {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    opacity: 0;
    background: rgba(0,0,0,0.6);
    border-radius: 24rpx;
    animation: width2big 0.5s 2.5s ease-in-out forwards;
}

@-webkit-keyframes width2big {
    from {
        opacity: 0;
        transform: scaleX(0);
    }

    to {
        opacity: 1;
        transform: scaleX(1);
    }
}

@keyframes width2big {
    from {
        opacity: 0;
        transform: scaleX(0);
    }

    to {
        opacity: 1;
        transform: scaleX(1);
    }
}

.add2myApp .wrap {
    position: relative;
    display: flex;
    align-items: center;
}

.add2myApp .close-icon {
    position: relative;
    width: 28rpx;
    height: 28rpx;
    opacity: 0;
    animation: closeSwing 2s 3s ease-out forwards;
    animation-timing-function: cubic-bezier(0,0.84,0.58,0.59);
}

.add2myApp .close-btn {
    position: absolute;
    left: -10rpx;
    top: 50%;
    padding: 12rpx;
    transform: translateY(-50%);
    width: 50rpx;
    height: 50rpx;
}

@-webkit-keyframes closeSwing {
    0% {
        opacity: 0;
    }

    10% {
        transform: rotate(160deg);
    }

    20% {
        transform: rotate(-140deg);
    }

    30% {
        transform: rotate(120deg);
    }

    40% {
        transform: rotate(-100deg);
    }

    50% {
        opacity: 0.4;
        transform: rotate(75deg);
    }

    60% {
        transform: rotate(-65deg);
    }

    70% {
        transform: rotate(40deg);
    }

    80% {
        transform: rotate(-22deg);
    }

    85% {
        transform: rotate(10deg);
    }

    90% {
        transform: rotate(-5deg);
    }

    95% {
        transform: rotate(2deg);
    }

    100% {
        opacity: 0.4;
        transform: rotate(0);
    }
}

@keyframes closeSwing {
    0% {
        opacity: 0;
    }

    10% {
        transform: rotate(160deg);
    }

    20% {
        transform: rotate(-140deg);
    }

    30% {
        transform: rotate(120deg);
    }

    40% {
        transform: rotate(-100deg);
    }

    50% {
        opacity: 0.4;
        transform: rotate(75deg);
    }

    60% {
        transform: rotate(-65deg);
    }

    70% {
        transform: rotate(40deg);
    }

    80% {
        transform: rotate(-22deg);
    }

    85% {
        transform: rotate(10deg);
    }

    90% {
        transform: rotate(-5deg);
    }

    95% {
        transform: rotate(2deg);
    }

    100% {
        opacity: 0.4;
        transform: rotate(0);
    }
}

